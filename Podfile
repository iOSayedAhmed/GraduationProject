
# Uncomment the next line to define a global platform for your project
 platform :ios, '9.0'

target 'ChatApp' do
  use_frameworks!

  # Pods for ChatApp

	# Firebase pods 

     pod 'Firebase/Core'
     pod 'Firebase/Auth'
     pod 'Firebase/Database'
     pod 'Firebase/Storage'
     pod 'Firebase/Crashlytics'
     pod 'Firebase/Analytics'
	
	#Facebook Pods 
     
     pod'FBSDKLoginKit'

	# Google Pod 
     pod'GoogleSignIn'

     pod'MessageKit'
     pod'JGProgressHUD'
     pod'RealmSwift'
     pod'SDWebImage'
end
